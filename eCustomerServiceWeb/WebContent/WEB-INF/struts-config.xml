<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
                               "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

	<!-- Data Sources -->
	<data-sources>
	</data-sources>

	<!-- Form Beans -->
	<form-beans>
		<form-bean name="logonForm" type="org.dhhs.dirm.acts.cs.formbeans.LogonForm" className="org.apache.struts.action.ActionFormBean">
		</form-bean>
