		<form-bean name="userForm" type="org.dhhs.dirm.acts.cs.formbeans.UserForm" className="org.apache.struts.action.ActionFormBean">
		</form-bean>
		<form-bean name="profileForm" type="org.dhhs.dirm.acts.cs.formbeans.ProfileForm" className="org.apache.struts.action.ActionFormBean">
		</form-bean>
		<form-bean name="referralTypeForm" type="org.dhhs.dirm.acts.cs.formbeans.ReferralTypeForm" className="org.apache.struts.action.ActionFormBean">
		</form-bean>
		<form-bean name="referralProcess" type="org.dhhs.dirm.acts.cs.formbeans.ReferralProcess" className="org.apache.struts.action.ActionFormBean">
		</form-bean>
		<form-bean name="manageTaskForm" type="org.dhhs.dirm.acts.cs.formbeans.ManageTaskForm" className="org.apache.struts.action.ActionFormBean">
